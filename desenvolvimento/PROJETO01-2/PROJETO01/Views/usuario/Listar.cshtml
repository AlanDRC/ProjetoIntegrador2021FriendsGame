@model List<PROJETO01.Modelos.usuario>

<h3>
    CADASTRO DE TIPOS DE JOGOS 

</h3>

<table class="table table-hover ">
    <thead>
        <tr>
            <th>NickJogador</th>
            <th>CodJogo</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.NickJogador</td>
                <td>@item.codJogo</td>

            </tr>
        }
    </tbody>
</table>
